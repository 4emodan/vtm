<style-line id="water:outline" stroke="#282d31" width="1.0" fix="true" cap="butt" />

<style-line id="water" stroke="#282d31" width="1.0" cap="butt" fix="true" />
<style-line id="river" use="water" stroke="#282d31" fix="false" />

